n index="3" value="341"/><column index="4" value="69"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TABLE IF NOT EXISTS `AdvancedTechniques` (`name` TEXT NOT NULL, `video` TEXT NOT NULL, `annotation` TEXT, PRIMARY KEY(`name`));
CREATE TABLE IF NOT EXISTS `Exercise` (`id` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `name` TEXT NOT NULL, `video` TEXT NOT NULL, `annotations` TEXT, `muscle` TEXT NOT NULL, FOREIGN KEY(`muscle`) REFERENCES `Muscle`(`name`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `Muscle` (`name` TEXT NOT NULL, PRIMARY KEY(`name`));
CREATE TABLE IF NOT EXISTS `Stretching` (`id` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `muscle` TEXT NOT NULL, `video` TEXT NOT NULL, `annotation` TEXT, FOREIGN KEY(`muscle`) REFERENCES `Muscle`(`name`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `Substitution` (`idExercise` INTEGER NOT NULL, `idExerciseSubstitute` INTEGER NOT NULL, PRIMARY KEY(`idExercise`, `idExerciseSubstitute`), FOREIGN KEY(`idExercise`) REFERENCES `Exercise`(`id`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`idExerciseSubstitute`) REFERENCES `Exercise`(`id`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `WeightHistoric` (`exerciseId` INTEGER NOT NULL, `trainingId` INTEGER NOT NULL, `date` INTEGER NOT NULL, `weight` TEXT NOT NULL, `substitutionId` INTEGER, `annotations` TEXT, PRIMARY KEY(`exerciseId`, `trainingId`, `date`), FOREIGN KEY(`exerciseId`) REFERENCES `Exercise`(`id`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`substitutionId`) REFERENCES `Exercise`(`id`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `Workout` (`id` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `identifier` INTEGER NOT NULL, `date` INTEGER NOT NULL, `hasPreWorkout` INTEGER NOT NULL, `focus` TEXT NOT NULL, `aerobic` TEXT NOT NULL);
CREATE TABLE IF NOT EXISTS `WorkoutExercise` (`id` INTEGER NOT NULL, `idWorkout` INTEGER NOT NULL, `idExercise` INTEGER NOT NULL, `series` TEXT NOT NULL, `advancedTechniqueName` TEXT, PRIMARY KEY(`id`), FOREIGN KEY(`idWorkout`) REFERENCES `Workout`(`id`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`idExercise`) REFERENCES `Exercise`(`id`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`advancedTechniqueName`) REFERENCES `AdvancedTechniques`(`name`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `WorkoutMuscles` (`idWorkout` INTEGER NOT NULL, `muscle` TEXT NOT NULL, PRIMARY KEY(`idWorkout`, `muscle`), FOREIGN KEY(`muscle`) REFERENCES `Muscle`(`name`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `WorkoutPlan` (`id` INTEGER NOT NULL, `workoutId` INTEGER NOT NULL, PRIMARY KEY(`id`, `workoutId`), FOREIGN KEY(`workoutId`) REFERENCES `Workout`(`id`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS room_master_table (id INTEGER PRIMARY KEY,identity_hash TEXT);</sql><current_tab id="0"/></tab_sql></sqlb_project>
